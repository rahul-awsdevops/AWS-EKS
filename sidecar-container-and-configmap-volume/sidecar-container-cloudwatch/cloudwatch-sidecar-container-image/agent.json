{
    "logs": {
      "logs_collected": {
        "files": {
          "collect_list": [
            {
              "file_path": "/var/log/nginx/access.log",
              "log_group_name": "nginx-access-logs",
              "log_stream_name": "nginx-access-1",
              "retention_in_days": 7
            },
            {
              "file_path": "/var/log/nginx/error.log",
              "log_group_name": "nginx-error-logs",
              "log_stream_name": "nginx-error-1",
              "retention_in_days": 7
            }
          ]
        }
      }
    }
  }
  